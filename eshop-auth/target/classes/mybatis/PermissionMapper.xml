<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.curise.eshop.auth.dao.PermissionDao">

    <select id="findByRoleId" resultType="com.curise.eshop.auth.entity.Permission" parameterType="Integer">
      select p.* from es_role_permission rp,es_permission p where rp.permission_id = p.id and rp.role_id = #{roleId,jdbcType=INTEGER}
    </select>


</mapper>